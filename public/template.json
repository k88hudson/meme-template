{
  "name": "default-butter-test",
  "snapshotHTMLOnReady": true,
  "editor": {
    "default": "butter/editors/default-editor.html",
    "googlemap": "js/plugins/googlemap/googlemap-editor.html"
  },
  "ui": {
    "trackEventHighlight": "click",
    "title": "mozilla Popcorn Meme Generator"
  },
  "plugin": {
    "plugins": [
      {
        "type": "footnote",
        "path": "butter/external/popcorn-js/plugins/footnote/popcorn.footnote.js"
      },
      {
        "type": "image",
        "path": "js/plugins/superimage/popcorn.image.js"
      },
      {
        "name": "flickr",
        "type": "flickr",
        "path": "js/plugins/flickr/popcorn.flickr.kenburns.js"
      },
      {
        "name": "zoink",
        "type": "zoink",
        "path": "js/plugins/zoink/popcorn.zoink.js"
      },
      {
        "name": "mediaspawner",
        "type": "mediaspawner",
        "path": "js/plugins/mediaspawner/popcorn.mediaspawner.js"
      },
      {
        "name": "meme",
        "type": "meme",
        "path": "js/plugins/meme/popcorn.meme.js"
      },
      {
        "name": "looper",
        "type": "looper",
        "path": "js/plugins/loop/popcorn.loop.js"
      },
      {
        "name": "soundOn",
        "type": "soundOn",
        "path": "js/plugins/soundOn/popcorn.soundOn.js"
      }
    ],
    "defaults": [
      "footnote",
      "image",
      "mediaspawner",
      "zoink",
      "meme",
      "looper",
      "soundOn"
    ]
  },
  "icons": {
    "default": "popcorn-icon.png",
    "image": "image-icon.png"
  },
  "dirs": {
    "popcorn-js": "butter/external/popcorn-js/",
    "css": "butter/css/",
    "dialogs": "butter/dialogs/",
    "resources": "butter/resources/"
  },
  "mediaDefaults": {
    "frameAnimation": true
  }
}

